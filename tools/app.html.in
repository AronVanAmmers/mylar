<!DOCTYPE html>
<html>
<head>
{{#each stylesheets}}  <link rel="stylesheet" href="{{this}}">
{{/each}}

<script type="text/javascript">
__meteor_runtime_config__ = "undefined";
function load_config() {
    xmlhttp = new XMLHttpRequest();
    //request has to be synchronous for config to affect other scripts
    xmlhttp.open("GET","{{config_url}}",false);
    xmlhttp.send(null);
    if (xmlhttp.readyState==4 && xmlhttp.status==200)
    {
       __meteor_runtime_config__ = JSON.parse(xmlhttp.responseText);
    }
}
load_config();
// ##RUNTIME_CONFIG##
</script>

{{#each scripts}}  <script type="text/javascript" src="{{this}}"></script>
{{/each}}

{{{head_extra}}}
</head>
<body>
{{{body_extra}}}
</body>
</html>
